    "import numpy as np\n",
    "\n",
    "lista = [[] for _ in range(3)]\n",
    "i = 0\n",
    "for x in np.arange(1, 10):\n",
    "    lista[i].append(x-1)\n",
    "    if x % 3 == 0: \n",
    "        i += 1\n",
    "        \n",
    "q3 = np.array(lista)\n",
    "print(q3)"
